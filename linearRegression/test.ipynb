    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# noise的分布符合高斯分布\n",
    "noise = np.random.normal(0, 1, size=100)\n",
    "x = np.linspace(-3, 3, 100)\n",
    "y = x**2 + 2 + noise\n",
    "plt.scatter(x, y)\n",
    "plt.show()"
   ]
